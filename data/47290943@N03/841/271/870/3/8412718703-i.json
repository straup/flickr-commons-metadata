r": "47290943@N03"}, {"raw": "Punchestown", "machine_tag": 0, "id": "47267889-8412718703-614174", "_content": "punchestown", "author": "47290943@N03"}, {"raw": "Watch Chains", "machine_tag": 0, "id": "47267889-8412718703-2046340", "_content": "watchchains", "author": "47290943@N03"}, {"raw": "National Library of Ireland", "machine_tag": 0, "id": "47267889-8412718703-4293432", "_content": "nationallibraryofireland", "author": "47290943@N03"}]}, "comments": {"_content": "40"}, "geoperms": {"isfriend": 0, "isfamily": 0, "ispublic": 1, "iscontact": 0}, "secret": "c77665e20a", "location": {"country": {"woeid": "23424803", "place_id": "SZmkZvhTUb5ZpbSWHw", "_content": "Ireland"}, "region": {"woeid": "2345257", "place_id": ".z1IHeZTUb4p8Wbm", "_content": "Kildare"}, "place_id": ".z1IHeZTUb4p8Wbm", "longitude": -6.518495, "context": "0", "woeid": "2345257", "latitude": 53.269396, "accuracy": "16"}, "usage": {"canblog": 0, "canshare": 1, "candownload": 1, "canprint": 0}, "description": {"_content": "Gentlemen at the Black Church Hotel in Kildare. We're fairly sure they were heading to or from a race meeting at Punchestown (see <a href=\"http://www.flickr.com/photos/nlireland/8422393425/\" target=\"_blank\"><b>Very Grand Stands</b></a> and <a href=\"http://www.flickr.com/photos/nlireland/8423486930/\" target=\"_blank\"><b>Roulette Wheel?</b></a>)...\n\nI'm tagging for Dandy, because if that isn't what that chap (complete with furled umbrella and nonchalant lean) is, then I'll eat the nearest stovepipe hat!\n\nPlus, is that a Landau anyone? Thinking it's time we learnt to tell our Landaus from our Broughams...\n\nDate: Circa 1868\n\nNLI Ref.: <a href=\"http://catalogue.nli.ie/Record/STP_1105\" target=\"_blank\" rel=\"nofollow\">STP_1105</a>"}, "isfavorite": 0, "views": "7223", "farm": 9, "visibility": {"isfriend": 0, "isfamily": 0, "ispublic": 1}, "rotation": 0, "dates": {"taken": "1868-00-01 00:00:00", "lastupdate": "1374054480", "takengranularity": "8", "posted": "1359105614"}, "originalsecret": "f2ebdcb6c9", "license": "7", "notes": {"note": [{"_content": "Dandy!", "author": "47290943@N03", "h": "91", "authorname": "National Library of Ireland on The Commons", "w": "29", "y": "311", "x": "396", "id": "72157632607720394"}, {"_content": "Landau?", "author": "47290943@N03", "h": "85", "authorname": "National Library of Ireland on The Commons", "w": "54", "y": "320", "x": "210", "id": "72157632603316677"}, {"_content": "D. Vickers\nBlackchurch Hotel", "author": "34695930@N00", "h": "25", "authorname": "_Harry Lime_", "w": "41", "y": "230", "x": "313", "id": "72157632603942807"}, {"_content": "Beano", "author": "14302637@N08", "h": "92", "authorname": "blackpoolbeach", "w": "31", "y": "311", "x": "359", "id": "72157632605497327"}]}, "server": "8184", "safety_level": "0", "urls": {"url": [{"type": "photopage", "_content": "http://www.flickr.com/photos/nlireland/8412718703/"}]}, "editability": {"canaddmeta": 0, "cancomment": 0}}, "stat": "ok"}