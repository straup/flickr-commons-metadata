ontent": "december", "author": "47290943@N03"}, {"raw": "1922", "machine_tag": 0, "id": "47267889-6581304207-29583", "_content": "1922", "author": "47290943@N03"}, {"raw": "1920s", "machine_tag": 0, "id": "47267889-6581304207-172", "_content": "1920s", "author": "47290943@N03"}, {"raw": "Twenties", "machine_tag": 0, "id": "47267889-6581304207-343849", "_content": "twenties", "author": "47290943@N03"}, {"raw": "Aspidistra", "machine_tag": 0, "id": "47267889-6581304207-212240", "_content": "aspidistra", "author": "47290943@N03"}, {"raw": "Poole Collection", "machine_tag": 0, "id": "47267889-6581304207-81069957", "_content": "poolecollection", "author": "47290943@N03"}, {"raw": "Glass Negative", "machine_tag": 0, "id": "47267889-6581304207-201865", "_content": "glassnegative", "author": "47290943@N03"}, {"raw": "A.H. Poole", "machine_tag": 0, "id": "47267889-6581304207-37482356", "_content": "ahpoole", "author": "47290943@N03"}, {"raw": "Arthur Henri Poole", "machine_tag": 0, "id": "47267889-6581304207-83069161", "_content": "arthurhenripoole", "author": "47290943@N03"}, {"raw": "Wristwatches", "machine_tag": 0, "id": "47267889-6581304207-666386", "_content": "wristwatches", "author": "47290943@N03"}, {"raw": "National Library of Ireland", "machine_tag": 0, "id": "47267889-6581304207-4293432", "_content": "nationallibraryofireland", "author": "47290943@N03"}]}, "comments": {"_content": "14"}, "geoperms": {"isfriend": 0, "isfamily": 0, "ispublic": 1, "iscontact": 0}, "secret": "fcfbe237d4", "location": {"locality": {"woeid": "561842", "place_id": "meHDqelUVLvxdjA", "_content": "New Ross"}, "country": {"woeid": "23424803", "place_id": "SZmkZvhTUb5ZpbSWHw", "_content": "Ireland"}, "region": {"woeid": "2345273", "place_id": "gI0mwAZTUb6aKUeJ", "_content": "Wexford"}, "place_id": "meHDqelUVLvxdjA", "longitude": -6.945569, "context": "0", "woeid": "561842", "latitude": 52.395568, "accuracy": "11"}, "usage": {"canblog": 0, "canshare": 1, "candownload": 1, "canprint": 0}, "description": {"_content": "The family of Mr. J.J. Kehoe of Ida House, New Ross, Co. Wexford.\n\nThe little girl on the left is holding <i>Bedtime Stories</i>, presumably a Poole Studios prop. She, like all of the girls, have embroidery on their clothes - wondering if this was all done at home?\n\nThe daughter/sister sitting in the chair at right looks studious, but on closer inspection, she's been captured &quot;reading&quot; a furniture catalogue, and I think she's wearing a wrist watch - don't think I've seen one in such an early photo before...\n\nDate: 27 December 1922\n\nNLI Ref.: <a href=\"http://catalogue.nli.ie/Record/P_WP_3080\" target=\"_blank\" rel=\"nofollow\">P_WP_3080</a>\n"}, "isfavorite": 0, "views": "5726", "farm": 8, "visibility": {"isfriend": 0, "isfamily": 0, "ispublic": 1}, "rotation": 0, "dates": {"taken": "1922-12-27 13:32:24", "lastupdate": "1374054427", "takengranularity": "0", "posted": "1324992744"}, "originalsecret": "7a591d9e58", "license": "7", "notes": {"note": [{"_content": "beautiful detail", "author": "18713525@N00", "h": "23", "authorname": "oana-emilia", "w": "39", "y": "167", "x": "273", "id": "72157628589547441"}, {"_content": "I hope she never lost her smile like her sisters seem to have.", "author": "37149688@N08", "h": "58", "authorname": "jake_sylvester", "w": "57", "y": "110", "x": "53", "id": "72157628612253987"}]}, "server": "7017", "safety_level": "0", "urls": {"url": [{"type": "photopage", "_content": "http://www.flickr.com/photos/nlireland/6581304207/"}]}, "editability": {"canaddmeta": 0, "cancomment": 0}}, "stat": "ok"}